<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox id="slotPane" fx:id="slotPane" styleClass="calendar-grid-cell" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1">
    <VBox alignment="TOP_LEFT" prefHeight="50" GridPane.columnIndex="0">
        <HBox alignment="CENTER_LEFT" spacing="5">
            <Label fx:id="title"  />
        </HBox>
        <Label fx:id="time" text="\$time" />
        <Label fx:id="description"  text="\$description" />
    </VBox>
</HBox>